<h1>New Song Form</h1>

<%= form_for @song do |f|%>
<%= f.label :name, value = "My song name:"%>
<%= f.text_field :name%>
<%= f.label :artist_id, value = "My song artist:"%>
<%= f.text_field :artist_id%>
<%= f.label :genre_id, value = "My song genre:"%>
<%= f.text_field :genre_id%>
<%= f.submit %>
<% end %>